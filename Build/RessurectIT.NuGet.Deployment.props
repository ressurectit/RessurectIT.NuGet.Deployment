<?xml version="1.0" encoding="utf-8"?>
<Project>
    <PropertyGroup>
        <NuGetServerApiKey Condition=" '$(NuGetServerApiKey)' == '' Or '$(NuGetServerApiKey)' == '*Undefined*' "></NuGetServerApiKey>
        <NuGetServerUrl Condition=" '$(NuGetServerUrl)' == '' Or '$(NuGetServerUrl)' == '*Undefined*' "></NuGetServerUrl>
        <NuGetSymbolServerApiKey Condition=" '$(NuGetSymbolServerApiKey)' == '' Or '$(NuGetSymbolServerApiKey)' == '*Undefined*' "></NuGetSymbolServerApiKey>
        <NuGetSymbolServerUrl Condition=" '$(NuGetSymbolServerUrl)' == '' Or '$(NuGetSymbolServerUrl)' == '*Undefined*' "></NuGetSymbolServerUrl>
        <NuGetExeFolders>$(NuGetPackageFolders)</NuGetExeFolders>
        <NuGetExePath>NuGet.CommandLine\6.0.0\tools\NuGet.exe</NuGetExePath>
        <PushScriptPath>$(MSBuildThisFileDirectory)..\internalTools\pushToRepository.ps1</PushScriptPath>
    </PropertyGroup>
</Project>